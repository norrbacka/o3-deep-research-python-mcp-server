# Azure AI Project Configuration
# Copy this file to .env and fill in your values

# Your Azure AI Project endpoint
# Format: https://<account>.services.ai.azure.com/api/projects/<project>
PROJECT_ENDPOINT=https://your-project.services.ai.azure.com/api/projects/your-project

# Bing resource connection name (as shown in Azure AI Foundry)
BING_RESOURCE_NAME=your-bing-resource

# Deep Research model deployment name
DEEP_RESEARCH_MODEL_DEPLOYMENT_NAME=o3-deep-research

# Base model deployment name (e.g., gpt-4o)
MODEL_DEPLOYMENT_NAME=gpt-4o

# Optional: Enable debug logging (remove or set to empty for production)
# DEBUG=1